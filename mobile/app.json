{
  "expo": {
    "name": "BoliBana Sugu",
    "slug": "sugu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bolibana.sugu"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.bolibana.sugu",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Autoriser l'accès à la caméra pour scanner les codes-barres"
        }
      ]
    ],
    "extra": {
      "apiBaseUrl": "https://www.bolibana.com/api",
      "eas": {
        "projectId": "408e7748-1d05-460b-92f4-0614d8b2a3c4"
      }
    },
    "owner": "pymalien"
  }
}
